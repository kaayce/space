{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "$schema"
  ],
  "tasks": {
    "build": {
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "public/**"
      ],
      "env": []
    },
    "lint": {
      "outputs": [],
      "inputs": [
        ".eslintrc.*",
        "src/**"
      ]
    },
    "type-check": {
      "outputs": [
        "*.tsbuildinfo"
      ]
    }
  }
}